  month = feb,
  year = {2015},
  keywords = {preregistration,replication,Bayes factor,exclude_no reference to rrs,registered-reports,default bayes factors,hypothesis,adversarial collaboration,enhance memory,handedness,horizontal eye movements,increased interhemispheric interaction,lexical memory,model,prior sensitivity,retrieval},
  pages = {E1-E15},
