  number = {8},
  journal = {Jmir Research Protocols},
  doi = {10.2196/resprot.9603},
  author = {{Martinez-Alcala}, Claudia I. and {Rosales-Lagarde}, Alejandra and {Hernandez-Alonso}, Esmeralda and {Melchor-Agustin}, Roberto and {Rodriguez-Torres}, Erika E. and {Itza-Ortiz}, Benjamin A.},
  month = aug,
