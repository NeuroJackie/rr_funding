  keywords = {cancer,exclude_no reference to rrs,mice,genes,landscape},
  pages = {e21634},
  file = {C\:\\Users\\Katie\\Zotero\\storage\\BUW7QVR2\\Horrigan et al. - 2017 - Replication Study Melanoma genome sequencing reve.pdf},
  note = {WOS:000393398100001}
}

